{
    "name": "silverslice/mail-reader",
    "description": "Test emails easily",
    "keywords": ["mail", "email", "test"],
    "license": "MIT",
    "authors": [
        {
            "name": "Igor Mikheev",
            "email": "silver_slice@mail.ru"
        }
    ],
    "require": {
        "php": ">=5.4",
        "zbateson/mail-mime-parser": "^0.2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4",
        "squizlabs/php_codesniffer" : "~2.3",
        "satooshi/php-coveralls": "~0.6",
        "symfony/filesystem": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Silverslice\\MailReader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Silverslice\\MailReader\\Tests\\": "tests/"
        }
    }
}
