{
    "name": "silverslice/mail-reader",
    "description": "Test emails easily",
    "keywords": ["mail", "email", "test"],
    "license": "MIT",
    "authors": [
        {
            "name": "Igor Mikheev",
            "email": "silver_slice@mail.ru"
        }
    ],
    "require": {
        "php": ">=7.2",
        "zbateson/mail-mime-parser": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "Silverslice\\MailReader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Silverslice\\MailReader\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/filesystem": "^6.0",
        "squizlabs/php_codesniffer": "^3.6",
        "satooshi/php-coveralls": "^1.0"
    }
}
